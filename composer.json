{
  "name": "internetrix/silverstripe-graphql-nested-mutations",
  "description": "Adds support for nested relationship mutations",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "module",
    "graphql",
    "mutations",
    "scaffolers"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Guy Watson",
      "email": "guy.watson@internetrix.com.au"
    }
  ],
  "require": {
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "~4.0",
    "silverstripe/graphql": "~1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
    "installer-name": "graphql-nested-mutations"
  },
  "autoload": {
    "psr-4": {
      "Internetrix\\GraphQLNestedMutations\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
