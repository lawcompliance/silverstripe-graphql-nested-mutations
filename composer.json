{
  "name": "internetrix/silverstripe-graphql-nested-mutations",
  "description": "Adds support for nested relationship mutations",
  "type": "silverstripe-vendormodule",
  "homepage": "https://gitlab.internetrix.net/silverstripe-modules/graphql-nested-mutations",
  "keywords": [
    "silverstripe",
    "module",
    "graphql",
    "mutations",
    "scaffolers"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Guy Watson",
      "email": "guy.watson@internetrix.com.au"
    }
  ],
  "support": {
    "issues": "https://gitlab.internetrix.net/silverstripe-modules/graphql-nested-mutations/issues"
  },
  "require": {
    "silverstripe/vendor-plugin": "^2",
    "silverstripe/framework": "^5",
    "silverstripe/graphql": "5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "10.2.6",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
    "installer-name": "graphql-nested-mutations"
  },
  "autoload": {
    "psr-4": {
      "Internetrix\\GraphQLNestedMutations\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
